{
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "auth0": {
          "registration": {
            "clientId": "YOUR_AUTH0_CLIENT_ID",
            "clientSecretSettingName": "AUTH0_CLIENT_SECRET",
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://YOUR_AUTH0_DOMAIN/.well-known/openid-configuration",
              "scopes": "openid profile email"
            }
          },
          "login": {
            "loginEndpoint": "https://YOUR_AUTH0_DOMAIN/authorize",
            "clientId": "YOUR_AUTH0_CLIENT_ID",
            "scopes": "openid profile email"
          },
          "logout": {
            "logoutEndpoint": "https://YOUR_AUTH0_DOMAIN/v2/logout",
            "clientId": "YOUR_AUTH0_CLIENT_ID"
          }
        }
      }
    }
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/protected/*",
      "allowedRoles": ["authenticated"]
    }
  ]
}
